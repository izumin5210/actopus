<% cell.lectures.each.with_index do |lecture, i| %>
  <li class="cell" style="<%= cell.styles(i) %>" data-toggle="popover" data-trigger="hover" data-html="true" title="<%= lecture.klass.name %>" data-content="<%= lecture.lecturers.map(&:name).join('<br>') %>">
    <%= link_to lecture.name, '#', class: 'cell-content', data: { id: lecture.id, name: lecture.name, class: lecture.klass.name, date: date.to_s, period: { id: cell.period.id, startTime: cell.start_time, endTime: cell.end_time }, lecturers: lecture.lecturers.map(&:name) } %>
  </li>
<% end %>
